{
	"name": "csv_datalake",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorageRawData",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"conteiner": {
				"type": "string"
			},
			"table_name": {
				"type": "string"
			},
			"extensao": {
				"type": "string"
			},
			"odate": {
				"type": "String"
			}
		},
		"folder": {
			"name": "Dados_internos"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat(\n    \ndataset().table_name,\n\n'_',\n\ndataset().odate,\n\ndataset().extensao\n\n)",
					"type": "Expression"
				},
				"container": {
					"value": "@dataset().conteiner",
					"type": "Expression"
				}
			},
			"columnDelimiter": ";",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}